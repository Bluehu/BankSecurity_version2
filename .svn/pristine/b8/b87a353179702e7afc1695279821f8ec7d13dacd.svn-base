}

.security .body .nav .nav-box {
    width: 190px;
    height: 712px;
    box-sizing: border-box;
    overflow: hidden;
}

.security .body .nav .nav-box ul {
    width: 190px;
    height: 702px;
    overflow-x: hidden;
    overflow-y: auto;
    margin-top: 10px;
}

.security .body .nav .nav-box ul li {
    width: 190px;
    height: 56px;
    zoom: 1;
    padding-left: 36px;
}

.security .body .nav .nav-box ul li.active {
    border-left: 4px solid #569ff1;
