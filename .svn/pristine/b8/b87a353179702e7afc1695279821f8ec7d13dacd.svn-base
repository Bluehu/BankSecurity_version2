    background-color: #303b50;
}

.security .body .nav .nav-box ul li:hover {
    border-left: 4px solid #569ff1;
    width: 150px;
    background-color: #303b50;
    cursor: pointer;
}

.security .body .nav .nav-box ul li:after {
    display: block;
    content: '';
